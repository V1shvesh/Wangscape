{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Wangscape terrain hypergraph schema",
  "description": "Specifies the clique membership of each terrain in a Wangscape configuration",
  "patternProperties":{
    "^[0-9a-zA-Z_-]+$": {
      "type": "array",
      "items":{
        "type": "array",
        "items": {
          "type": "string",
          "pattern": "^[0-9a-zA-Z_-]+$"
        },
        "minItems": 1
      }
    }
  }
}